<?xml version="1.0" encoding="utf-8"?>
<resources>
	<!--	<style name="Theme.NupyAndroid" parent="android:Theme.Material.Light.NoActionBar" />-->

	<style name="Theme.NupyAndroid" parent="Theme.MaterialComponents.DayNight.NoActionBar">
		<!-- Primary brand color. -->
		<item name="colorPrimary">@color/gray200</item>
		<item name="colorPrimaryVariant">@color/black</item>
		<item name="colorOnPrimary">@color/white</item>
		<!-- Secondary brand color. -->
		<item name="colorSecondary">@color/teal_200</item>
		<item name="colorSecondaryVariant">@color/teal_700</item>
		<item name="colorOnSecondary">@color/black</item>
		<!-- Status bar color. -->
		<item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
		<!-- Customize your theme here. -->
	</style>

	<style name="Theme.NupyAndroid.NoActionBar">
		<item name="windowActionBar">false</item>
		<item name="windowNoTitle">true</item>
	</style>

	<style name="Theme.Tinji.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

	<style name="Theme.Tinji.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>